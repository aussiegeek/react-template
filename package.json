{
  "name": "react-template",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.html",
    "lint": "yarn eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix",
    "lint:check": "yarn eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix",
    "start": "parcel serve src/index.html",
    "test": "yarn jest",
    "typecheck": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json,yaml}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "parcel": "2.0.0-nightly.429",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@aussiegeek/eslint-config-react": "0.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jest-formatting": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "sort-package-json": "1.52.0",
    "ts-jest": "26.5.6"
  }
}
